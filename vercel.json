{
    "version": 2,
    "builds": [
        { "src": "bookstore/wsgi.py", "use": "@vercel/python", "config": { "runtime": "python3.11" } },
        { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "distDir": ".vercel/output" } }
    ],
    "routes": [
        { "src": "/static/(.*)", "dest": "/static/$1" },
        { "src": "/(.*)", "dest": "bookstore/wsgi.py" }
    ]
}